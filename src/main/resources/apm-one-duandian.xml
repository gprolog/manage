<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="edit.duandian" namespace="/" extends="struts-default">
		<global-results>
			<result name="ERROR">
				/WEB-INF/pages/error/error.jsp
			</result>
			<result name="login">
				/WEB-INF/pages/login/loginIndex.jsp
			</result>
		</global-results>
		<action name="duandian_*" method="{1}"
			class="com.oneapm.web.info.DuandianAction">
			<result name="index">
				/WEB-INF/pages/duandian/duandian_index.jsp
			</result>
		</action>
	</package>
</struts>